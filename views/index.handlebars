        <a class="py-2 d-none d-md-inline-block" href="views/admin.handlebars">Login</a>
      </div>
    </nav>

    <div class="position-relative overflow-hidden p-3 p-md-5 m-md-3 text-center bg-light">
      <img src="https://files.slack.com/files-pri/TAXHPJA0Z-FEJ7VC542/2.jpg" class="img-fluid" alt="Responsive image" style="background-size: cover;">
      <div class="py-5 text-center">
        <img class="d-block mx-auto mb-4" src="../../assets/brand/bootstrap-solid.svg" alt="" width="72" height="72">
        <h2>{{msg}}</h2>
